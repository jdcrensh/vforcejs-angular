{
  "name": "vforcejs-angular",
  "version": "0.1.0",
  "description": "A working template for building an Angular SPA on a Salesforce public site",
  "private": true,
  "scripts": {
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf ./build/*",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "start": "npm run start:tunnel",
    "start:tunnel": "cross-env NODE_ENV=development node internals/server",
    "lint": "npm run lint:js",
    "lint:js": "npm run lint:eslint && npm run lint:jscs",
    "lint:js:staged": "npm run lint:eslint:staged && npm run lint:jscs:staged",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:eslint:staged": "eslint-staged",
    "lint:jscs": "jscs .",
    "lint:jscs:staged": "jscs-staged",
    "lint:staged": "npm run lint:js:staged",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redteal/rf-account-editor.git"
  },
  "author": "Jon Crenshaw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redteal/rf-account-editor/issues"
  },
  "homepage": "https://github.com/redteal/rf-account-editor",
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-cookies": "^1.5.5",
    "angular-es6": "^3.0.5",
    "angular-resource": "^1.5.5",
    "angular-sanitize": "^1.5.5",
    "angular-touch": "^1.5.5",
    "angular-ui-bootstrap": "^1.3.3",
    "angular-ui-router": "^0.3.0",
    "bootstrap-sass": "^3.3.6",
    "core-js": "^2.4.0",
    "es6-promisify": "^4.1.0",
    "font-awesome": "^4.6.3",
    "history": "^2.1.1",
    "immutable": "^3.8.1",
    "loglevel": "^1.4.0",
    "oclazyload": "^1.0.9",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "archiver": "^1.0.0",
    "async": "^2.0.0-rc.5",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^3.0.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "inherits": "^2.0.1",
    "ip": "^1.1.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jsforce": "^1.6.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "mustache": "^2.2.1",
    "mustache-loader": "^0.3.1",
    "ng-annotate-loader": "^0.1.0",
    "ngrok": "^2.1.8",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-cssnext": "^2.5.2",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stream-buffers": "^3.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-split-by-path": "0.0.8",
    "winston": "^2.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "import/no-unresolved": 2,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-console": 1,
      "no-unused-expressions": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-shadow": 0,
      "max-len": 0,
      "prefer-template": 2,
      "no-use-before-define": 0,
      "newline-per-chained-call": 0,
      "global-require": 0,
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "jsx-a11y/aria-props": 2
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.dev.babel.js"
        }
      }
    }
  },
  "jscsConfig": {
    "preset": "airbnb",
    "validateQuoteMarks": null,
    "requireTrailingComma": null,
    "maximumLineLength": null,
    "maxErrors": null,
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "requireShorthandArrowFunctions": null,
    "requirePaddingNewLinesBeforeLineComments": null,
    "requirePaddingNewLinesAfterBlocks": null,
    "excludeFiles": [
      "build/**",
      "coverage/**",
      "node_modules/**"
    ]
  }
}
